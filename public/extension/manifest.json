{
    "manifest_version": 3,
    "name": "Browser History Tracker",
    "version": "1.0",
    "permissions": [
      "history",      
      "storage"       
    ],
    "background": {
      "service_worker": "background.js"  
    },
    "action": {
      "default_popup": "popup.html"     
    }
  }
  